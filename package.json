{
    "name": "vintage-time",
    "version": "1.3.5",
    "description": "DateTime x DateOnly library with locale support. Compatible with sequelize, joi, momentjs and plain javascript Dates",
    "type": "commonjs",
    "main": "index.cjs",
    "module": "index.mjs",
    "engines": {
        "node": ">=18.0.0"
    },
    "exports": {
        ".": {
            "require": "./index.cjs",
            "default": "./index.mjs"
        },
        "./regex": {
            "require": "./regex.cjs",
            "default": "./regex.mjs"
        },
        "./locale-formats": {
            "require": "./locale-formats.cjs",
            "default": "./locale-formats.mjs"
        },
        "./plugins/joi": {
            "require": "./plugins/joi.cjs",
            "default": "./plugins/joi.mjs"
        },
        "./plugins/sequelize": {
            "require": "./plugins/sequelize.cjs",
            "default": "./plugins/sequelize.mjs"
        }
    },
    "scripts": {
        "test": "jest --forceExit --detectOpenHandles --coverage"
    },
    "keywords": [
        "date",
        "time",
        "date-time",
        "date-only",
        "timezone",
        "locale",
        "calendar",
        "moment",
        "sequelize",
        "joi"
    ],
    "author": "metadeta96",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fernando7jr/vintage-time.git"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "joi": "17.x",
        "prettier": "^3.2.5",
        "sequelize": "6.x",
        "sqlite3": "^5.1.7"
    },
    "dependencies": {
        "moment-timezone": "^0.5.45"
    }
}
